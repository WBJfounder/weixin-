<swiper class="star"  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}"  wx:for-index="index">
    <swiper-item>
      <image   src="{{item}}" class="slide-image" width="355" height="150"/>
      <button bindtap="into" plain="true" class="btn" wx:if="{{index==imgUrls.length-1}}">进入</button>
    </swiper-item>
  </block>
</swiper>